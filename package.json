        "@types/chrome": "0.0.112",
        "@types/jquery": "^3.3.38",
        "@types/moment": "^2.13.0",
        "@types/node": "^14.0.9",
        "@types/sprintf-js": "^1.1.2",
        "datatables": "^1.10.18",
        "jquery": "^3.5.1",
        "jsdom": "^16.2.2",
        "lzjs": "^1.3.0",
        "mocha": "^7.2.0",
        "moment": "^2.26.0",
